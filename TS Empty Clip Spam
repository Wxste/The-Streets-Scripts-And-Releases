                wait(.1)game:GetService("Players").LocalPlayer.Backpack.Glock.Click:FireServer()
                wait(.1)game:GetService("Players").LocalPlayer.Backpack.Glock.Click:FireServer()
                wait(.1)game:GetService("Players").LocalPlayer.Backpack.Glock.Click:FireServer()
                wait(.1)game:GetService("Players").LocalPlayer.Backpack.Glock.Click:FireServer()
                wait(.1)game:GetService("Players").LocalPlayer.Backpack.Glock.Click:FireServer()
